  color: var(--px-clr-white-1);
  transition: all 0.5s var(--px-cube-1);
  display: inline-flex;
  align-items: center;
  gap: 22px;
}
.pf-pr-1 .icon {
  width: 32px;
  height: 32px;
  border-radius: 50%;
  background: var(--px-clr-white-1);
  fill: transparent;
  position: relative;
  z-index: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  transition: all 0.5s var(--px-cube-1);
  overflow: hidden;
}
.pf-pr-1 .icon svg {
  transition: all 0.5s var(--px-cube-1);
}
.pf-pr-1 .icon svg:nth-child(2) {
  position: absolute;
  transform: translate(-20px, 20px);
}
.pf-pr-1 .icon svg:nth-child(2) path {
  stroke: var(--px-clr-white-1);
}
.pf-pr-1 .text {
  position: relative;
  z-index: 1;
  overflow: hidden;
}
.pf-pr-1 .text::after {
  top: 0;
  left: 0;
  opacity: 0;
  width: 100%;
  display: block;
  transition: all 0.5s var(--px-cube-1);
  position: absolute;
  content: attr(data-back);
  transform: translateY(100%);
}
.pf-pr-1 .text::before {
  top: 0;
  left: 0;
  opacity: 1;
  display: block;
  transition: all 0.5s var(--px-cube-1);
  position: relative;
  content: attr(data-front);
  transform: translateY(0);
}
.pf-pr-1:hover {
  background: var(--px-clr-sd-1);
  color: var(--px-clr-white-1);
}
.pf-pr-1:hover .icon {
  background: var(--px-clr-pr-1);
}
.pf-pr-1:hover .icon svg:nth-child(1) {
  transform: translate(20px, -20px);
}
.pf-pr-1:hover .icon svg:nth-child(2) {
  transform: translate(0px, 0px);
}
.pf-pr-1:hover .text::after {
  opacity: 1;
  transform: translateY(0%);
}
.pf-pr-1:hover .text::before {
  opacity: 0;
  transform: translateY(-120%);
}

