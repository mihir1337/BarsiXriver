.pf-pr-1 {
  position: relative;
  z-index: 1;
  flex: 0 0 auto;
  font-family: var(--px-font-1);
  text-transform: capitalize;
  font-size: 16px;
  line-height: 1.5;
  border: 1.5px solid transparent;
  background-color: var(--px-clr-pr-1);
  letter-spacing: 0.5px;
  font-weight: 600;
  border-radius: 100px;
  padding: 11.5px 12px;
