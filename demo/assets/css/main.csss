:root {
  scroll-behavior: inherit;
  --px-font-1: "Inter", sans-serif;
  --px-clr-pr-1: #FF2759;
  --px-clr-sd-1: #8B5BFA;
  --px-clr-h-1: #0A0E0E;
  --px-clr-p-1: #212527;
  --px-clr-white-1: #ffffff;
  --px-clr-black-1: #000000;
  --px-cube-1: cubic-bezier(0.19, 1, 0.22, 1);
}

